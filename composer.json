{
    "name": "ta-salt/mink-selenium4-driver",
    "description": "Selenium4 (WebDriver) driver for Mink framework",
    "keywords": ["selenium", "webdriver", "javascript", "ajax", "testing", "browser"],
    "homepage": "https://mink.behat.org/",
    "type": "mink-driver",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Pete Otaqui",
            "email": "pete@otaqui.com",
            "homepage": "https://github.com/pete-otaqui"
        },
        {
            "name": "Konstantin Kudryashov",
            "email": "ever.zet@gmail.com",
            "homepage": "http://everzet.com"
        },
        {
            "name": "Shu Chen",
            "email": "shu@ediasoft.com",
            "homepage": "https://ediasoft.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "behat/mink": "^1.11@dev",
        "instaclick/php-webdriver": "dev-master"
    },
    "require-dev": {
        "mink/driver-testsuite": "dev-master",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^8.5.22 || ^9.5.11",
        "symfony/error-handler": "^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Behat\\Mink\\Driver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Behat\\Mink\\Tests\\Driver\\": "tests"
        }
    }
